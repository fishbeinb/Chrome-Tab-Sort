{
  "name": "Simple tab Organizer",
  "description": "Groups chrome tabs by domain",
  "version": "1.0",
  "background": { 
	"scripts": ["background.js"] 
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "name": "Organize"
  },
  "manifest_version": 2
}
